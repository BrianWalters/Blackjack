resets = true

@import '../../source/blackjack'

.box
    align-items center
    border 2px solid #333
    display flex
    justify-content center
    height 100px
    width @height

    &.top-right
        @extend $topright

    &.top-left
        @extend $topleft

    &.bottom-left
        @extend $bottomleft

    &.bottom-right
        @extend $bottomright